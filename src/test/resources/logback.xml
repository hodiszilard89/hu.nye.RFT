<?xml version="1.0" encoding="UTF-8"?>

<configuration>

  <appender name="ReportPortalAppender" class="com.epam.reportportal.logback.appender.ReportPortalAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

</configuration>